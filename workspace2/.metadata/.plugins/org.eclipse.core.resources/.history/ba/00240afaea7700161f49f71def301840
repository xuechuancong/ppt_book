package study;

public class Isprime {
	//第一个函数判断一个数是否为素数
	bool IsSu(int number){
	    if(number==2 && number==3)
	        return true;
	    for(int i=2;i<=sqrt(number);i++){
	        if(number%i==0){
	            return false;
	        }
	    }
	    return true;
	}
	void printSu(){
	    for(int i=2;i<=2000;i++){
	        if(IsSu(i))
	            printf("%d ",i);
	    }
	    printf("\n");
	}

}
